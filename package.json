{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Esto es Webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-styles-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:url-loader": "webpack-dev-server  --config ./url-loader/webpack.config.js",
    "build:prepro": "webpack-dev-server  --config ./prepro/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@nikosantis",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.13.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "url-loader": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  }
}
